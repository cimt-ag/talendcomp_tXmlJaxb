<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
    <HEADER 
    	AUTHOR="cimt AG" 
    	COMPATIBILITY="ALL"
        SCHEMA_AUTO_PROPAGATE="false"
        PLATEFORM="ALL" 
        RELEASE_DATE="20161010"
        SERIAL="" 
        STARTABLE="true" 
        STATUS="PROD" 
        VERSION="${project.version}">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>XML</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="0"
                           MIN_OUTPUT="0" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
        <CONNECTOR CTYPE="COMPONENT_OK" />
        <CONNECTOR CTYPE="COMPONENT_ERROR" />
        <CONNECTOR CTYPE="RUN_IF" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER 
            NAME="XSD_FILE" 
            FIELD="FILE" 
            NUM_ROW="10" 
        />

        <PARAMETER 
            NAME="MODELCACHE_DIR"
            NUM_ROW="20"
            FIELD="DIRECTORY"
            REQUIRED="true" 
        >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		
        <PARAMETER 
            NAME="CREATE_JAR_FILE" 
            FIELD="CHECK" 
            NUM_ROW="30" 
            REQUIRED="true" 
        />

        <PARAMETER 
            NAME="JAR_FILE" 
            FIELD="FILE" 
            NUM_ROW="40" 
            REQUIRED="true"
            SHOW_IF="CREATE_JAR_FILE == 'true'" 
        />

    </PARAMETERS>

    <ADVANCED_PARAMETERS>
    
        <PARAMETER 
            NAME="PRINTOUT_ABSTRACT_CLASSES" 
            FIELD="CHECK" 
            NUM_ROW="30" 
            REQUIRED="true" 
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        
        <PARAMETER 
            NAME="PRINTOUT_DOCUMENT_GRAPH" 
            FIELD="CHECK" 
            NUM_ROW="40" 
            REQUIRED="true" 
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER 
            NAME="PERFORM_TYPE_SUBSTITUTE" 
            FIELD="CHECK" 
            NUM_ROW="50" 
            REQUIRED="true" 
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT NAME="cimt-talendcomp-xmldynamic" MODULE="${build.finalName}.jar" REQUIRED="true" />
            <IMPORT NAME="colllib" MODULE="colllib-1.6.jar" REQUIRED="true" />
            <IMPORT NAME="commons-beanutils-core" MODULE="commons-beanutils-core-1.8.3.jar" REQUIRED="true" />
            <IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
            <IMPORT NAME="jaxb-core" MODULE="jaxb-core-2.2.11.jar" REQUIRED="true" />
            <IMPORT NAME="jaxb-xjc" MODULE="jaxb-xjc-2.2.11.jar" REQUIRED="true" />
            <IMPORT NAME="juel-api" MODULE="juel-api-2.2.2.jar" REQUIRED="true" />
            <IMPORT NAME="juel-impl" MODULE="juel-impl-2.2.2.jar" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS>
        <RETURN AVAILABILITY="AFTER" NAME="XSD_FILE" TYPE="id_String" />
        <RETURN AVAILABILITY="AFTER" NAME="MODELCACHE_DIR" TYPE="id_String" />
        <RETURN AVAILABILITY="AFTER" NAME="JAR_FILE" TYPE="id_String" />
    </RETURNS>
</COMPONENT>
	