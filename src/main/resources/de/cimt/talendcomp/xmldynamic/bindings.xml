<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" 
          xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
          version="2.0">
        
    <jaxb:globalBindings
        fixedAttributeAsConstantProperty="true"
        typesafeEnumBase="xsd:string"
    >
       
        <xjc:simple />
        <xjc:javaType name="java.util.Date" xmlType="xsd:date"     adapter="de.cimt.talendcomp.xmldynamic.adapter.DateAdapter" />
        <xjc:javaType name="java.util.Date" xmlType="xsd:gYearMonth" adapter="de.cimt.talendcomp.xmldynamic.adapter.DateAdapter" />
        
        <xjc:javaType name="java.util.Date" xmlType="xsd:dateTime" adapter="de.cimt.talendcomp.xmldynamic.adapter.DateTimeAdapter" />
        
    </jaxb:globalBindings>
       
        
</jaxb:bindings> 